<?xml version="1.0" encoding="utf-8"?>
<resources>
        <declare-styleable name="MenuItemView">
            <attr name="drawLeft" format="reference"></attr>
            <attr name="drawRight" format="reference"></attr>
            <attr name="text" format="string"></attr>
            <attr name="textSize" format="dimension"></attr>
            <attr name="textColor" format="color"></attr>
            <attr name="textPaddLeft" format="dimension"></attr>
            <attr name="lineColor" format="color"></attr>
            <attr name="lineBackground" format="color"></attr>
            <attr name="lineSize" format="dimension"/>
            <attr name="lineStart" >
                <flag name="padd" value="0"/>
                <flag name="draw" value="1"/>
                <flag name="text" value="2"/>
                <flag name="view" value="3"/>
            </attr>
            <attr name="lineEnd" >
                <flag name="padd" value="0"/>
                <flag name="draw" value="1"/>
                <flag name="text" value="2"/>
                <flag name="view" value="3"/>
            </attr>
        </declare-styleable>
    <declare-styleable name="TSeekBar">
        <attr name="textsize" format="dimension" />
        <attr name="textcolor" format="color" />
        <attr name="img" format="reference" />
    </declare-styleable>
</resources>